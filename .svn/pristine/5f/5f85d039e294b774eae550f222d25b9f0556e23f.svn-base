package com.vmware.scan.analyzer.main.pojos;

public enum SplittingSymbols {

	AMPERSAND("&"),
	ARROW("=>"),
	COLON(":"),
	SEMICOLON(";"),
	COMMA(","),
	EQUALSTO("=");
	
	private String symbol;
	
	private SplittingSymbols(String symbol){
		this.symbol = symbol;
	}
	
	public String getValue(){
		return this.symbol;
	}
	
}
